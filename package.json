{
  "name": "ngx.selfbits.io",
  "version": "1.4.0",
  "description": "Angular typescript client for Selfbits Cloud Platform",
  "author": "Selfbits GmbH",
  "keywords": [
    "selfbits",
    "typescript",
    "angular2"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "build": "rm -rf dist/* && ngc --outDir dist/",
    "commit": "git-cz",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "check-version": "semantic-release pre"
  },
  "peerDependencies": {
    "@angular/core": "^4.0.0",
    "@angular/http": "^4.0.0"
  },
  "devDependencies": {
    "@angular/common": "4.1.2",
    "@angular/compiler": "4.1.2",
    "@angular/compiler-cli": "4.1.2",
    "@angular/core": "4.1.2",
    "@angular/http": "4.1.2",
    "@angular/platform-browser": "4.1.2",
    "@angular/platform-server": "4.1.2",
    "@types/node": "7.0.18",
    "awesome-typescript-loader": "3.1.3",
    "commitizen": "2.9.6",
    "core-js": "^2.4.0",
    "cz-conventional-changelog": "1.2.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.1",
    "semantic-release": "^6.3.6",
    "tslint": "5.2.0",
    "tslint-eslint-rules": "4.1.1",
    "tslint-loader": "3.5.3",
    "typescript": "2.2.1",
    "typings": "^1.3.2",
    "webpack": "2.5.1",
    "zone.js": "^0.8.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "debug": false,
    "verifyConditions": {
      "path": "./node_modules/semantic-release/src/lib/plugin-noop.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/selfbits/ngx.selfbits.io.git"
  }
}